insert into data_diff_boss_business_rule
select user_id,1,'boss_user','固话用户用户类型错误' from 
 boss_user_type_repair_&1 a where  exists
(
select/*parallel(b,5)*/ 1 from aicbs.boss_user b where a.user_id=b.user_id
and b.user_type=33
);
commit;
