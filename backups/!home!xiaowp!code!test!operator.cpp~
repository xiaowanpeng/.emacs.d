#include <iostream>
#include <string>

using namespace std;

class A
{
    public:
        friend A& operator << (A& a, const string& str);
        string get_str() {return m_str;};
    private:
        string m_str;
        
};

A& operator << (A& a, const string& str)
{
    a.m_str += str;
    return a;
}

int main()
{
    A a;
    a << "abc" << "123";
    cout << a.get_str() << endl;
    return 0;
}
