#include <iostream>

using namespace std;

class A
{
    public:
        virtual void run()
            {
                cout << "A" << endl;
            };
};

class B: public A
{
    public:
       virtual void run()
            {
                A::run();
                cout << "B" << endl;
            };
};

int main()
{
    A* a = new B();

    a->run();
    return 0;
}
