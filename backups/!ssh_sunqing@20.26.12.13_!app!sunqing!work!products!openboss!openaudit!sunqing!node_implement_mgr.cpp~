#include "node_implement_mgr.h"
#include <iostream>
//实现某个业务类 begin
class CImplNode : public CNodeAbstractBase
{
public:
//声明需要派生类实现的接口
         int init()
        {
          std::cout<<"initialize the interface success.";
        };
         int run()
        {
          std::cout<<"run the interface success.";
        };
         int redo()
        {

          std::cout<<"redo the interface success.";
        };
         int resume()
        {
  
          std::cout<<"resume the interface success.";
        };
         int pause()
        {

          std::cout<<"pause the interface success.";
        };
         int stop()
        {

          std::cout<<"stop the interface success.";
        };
         int clean()
        {

          std::cout<<"clean the interface success.";
        };
};


extern "C"  CNodeAbstractBase    * New() {
    return new CImplNode;
}
extern "C" void Delete( CNodeAbstractBase    * p) {
    delete p;
}


//实现某个业务类 end
