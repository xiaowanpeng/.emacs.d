std::auto_ptr< CDmcextTemplateMgr<MFlowManagerDb::CFmFlowDefineList> > CDmcextAuditMgr::getFmFlowDefine(SOBSession* pSession)
{
    std::auto_ptr< CDmcextTemplateMgr<MFlowManagerDb::CFmFlowDefineList> > m_pFmFlowDefine ( 
        new CDmcextTemplateMgr<MFlowManagerDb::CFmFlowDefineList>(pSession, DB_JH, SCHEME_NAME_AIJH));
    return m_pFmFlowDefine;
}

