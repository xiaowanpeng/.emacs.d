    map<int, string> mp;
    mp[1] = "1";
    mp[2] = "2";

    map<int, string>::iterator itr;
    for (itr = mp.begin(); itr != mp.end(); itr ++)
    {
        cout << itr->first << " : " << itr->second << endl;
    }

    mp.erase(mp.find(1));

    for (itr = mp.begin(); itr != mp.end(); itr ++)
    {
        cout << itr->first << " : " << itr->second << endl;
    }

    return 0;
}
