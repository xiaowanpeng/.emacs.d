#include "base_db_func.h"


std::auto_ptr< CDmcextTemplateMgr<CFmFlowDefineList> > CDmcextAuditMgr::getFmFlowDefine(SOBSession* pSession)
{
    std::auto_ptr< CDmcextTemplateMgr<CFmFlowDefineList> > m_pFmFlowDefine ( 
        new CDmcextTemplateMgr<CFmFlowDefineList>(pSession, DB_JH, SCHEME_NAME_AIJH));
    return m_pFmFlowDefine;
}

