#!/bin/sh
checkout(){
	if [ `ps -ef |grep AuditAppServer |grep -v grep|grep -v start_AuditAppServer.sh |wc -l` -gt 0 ]
		then
		echo 1
	else 
		echo 0
	fi
}
checkout
sleep 1
if [ `checkout`  -eq 0 ]
then
	$HOME/bin/AuditAppServer -i $HOME/config/AuditAppServer.cfg -m AuditAppServer > /dev/null &
else
	echo "The process of AuditAppServer have been started !" ;exit
fi
sleep 1
if [ `checkout` -eq 0 ]
then 
	echo "The process of AuditAppServer has start failed !"
else
	echo "The process of AuditAppServer have been started !" 
fi
