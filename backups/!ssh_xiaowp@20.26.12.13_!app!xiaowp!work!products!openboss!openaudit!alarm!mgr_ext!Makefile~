	DMCPublic$(BUILDTYPE) 					\
	DMCTinyXml$(BUILDTYPE)           \
	luainterface$(BUILDTYPE)                \
	luabase$(BUILDTYPE)                        

USER_LIBRARIES = CbsSysClass$(BUILDTYPE)	\
	OpenBuffer$(BUILDTYPE)					\
	des$(BUILDTYPE)							\
	DataAuditMgnt$(TIER_KERNEL)$(BUILDTYPE)	\
	DMCPublic$(BUILDTYPE) 					\
	DMCTinyXml$(BUILDTYPE)           \
	luainterface$(BUILDTYPE)                \
