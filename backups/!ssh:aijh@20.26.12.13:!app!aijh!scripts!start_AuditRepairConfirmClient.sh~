#!/bin/sh
checkout(){
	if [ `ps -ef |grep AuditDSClient |grep -v grep|grep 'AuditRepairClient.cfg -m REPAIR'|grep -v start_AuditRepairConfirmClient.sh |wc -l` -gt 0 ]
		then
		echo 1
	else 
		echo 0
	fi
}
checkout
sleep 1
if [ `checkout`  -eq 0 ]
then
	$HOME/bin/AuditDSClient -i $HOME/config/AuditRepairClient.cfg -m REPAIR_CONFIRM > /dev/null &
else
	echo "The process of AuditRepairConfirmClient have been started !" ;exit
fi
sleep 1
if [ `checkout` -eq 0 ]
then 
	echo "The process of AuditRepairConfirmClient has start failed !"
else
	echo "The process of AuditRepairConfirmClient have been started !" 
fi
