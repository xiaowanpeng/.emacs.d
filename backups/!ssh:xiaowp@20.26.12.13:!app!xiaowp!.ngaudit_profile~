echo "  3. initcode : 根据OBD生成基础代码                 "
echo "****************************************************"
echo "* gmake 安装参数说明:                              *"
echo "****************************************************"
echo "  1. gmake install_sys "
echo "     用途: 将动态库和服务安装到公共环境 "
echo "     将动态库安装到:   [$OB_REL/lib] "
echo "     将服务安装到:     [$OB_REL/svr] "
echo "  2. gmake install_user"
echo "     用途: 将动态库和服务安装到自测环境 "
echo "     将动态库安装到:   [$HOME/lib] "
echo "     将服务安装到:     [$HOME/svr] "
echo "  3. gmake install "
echo "     执行 gmake install_user 和 gmake install_sys "
echo "     搜索路径:   [$HOME/lib] 优先于 [$OB_REL/lib] "
echo "                 [$HOME/svr] 优先于 [$OB_REL/svr] "
echo "  4. gmake install_headers_sys "
echo "     用途: 将头文件安装到公共环境 "
echo "     将头文件安装到:   [$OB_REL/include] "
echo "  5. gmake install_headers_user"
echo "     用途: 将头文件安装到自测环境 "
echo "     将头文件安装到:   [$HOME/include] "
echo "  6. gmake install_headers "
echo "     执行 gmake install_headers_user 和 gmake install_headers_sys "
echo "     搜索路径:   [$HOME/include] 优先于 [$OB_REL/include] "
echo "****************************************************"
echo "Hello, Welcome to linux development environment!    "
echo "  openaudit: new audit project 2014"
echo "  opendmc  : old audit project 2012"
