#include <iostream>

using namespace std;

class C
{
public:
    int a;
    int b;

    bool operator < (C& c)
    {
        if (this->a < c.a)
        {
            return true;
        }
        else if (this->a == c.a)
        {
            if (this->b < c.b)
            {
                return true;
            }
            return false;
        }
        else
        {
            return false;
        }
    };
};

int main(int argc, char *argv[])
{
    
    return 0;
}
