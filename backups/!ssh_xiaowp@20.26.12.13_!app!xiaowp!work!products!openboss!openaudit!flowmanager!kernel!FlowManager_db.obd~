        datetime BEGIN_DATE;
        datetime END_DATE;
        int16   sts;
        string  RUN_MSG;
        string  REGION_CODE;
        int32   BUSI_ID;
        string  XML;
    };
    list vector<CFmTask> CFmTaskList;
    
    struct CFmTaskLog import table FM_TASK_LOG
    {
    };
    list vector<CFmTaskLog> CFmTaskLogList;


